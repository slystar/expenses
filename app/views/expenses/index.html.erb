<h1>Listing expenses</h1>

<table>
  <tr>
    <th>Date purchased</th>
    <th>Description</th>
    <th>Pay method</th>
    <th>Reason</th>
    <th>Store</th>
    <th>User</th>
    <th>Group</th>
    <th>Amount</th>
    <th>Process date</th>
    <th>Process flag</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @expenses.each do |expense| %>
  <tr>
    <td><%= expense.date_purchased %></td>
    <td><%= expense.description %></td>
    <td><%= expense.pay_method_id %></td>
    <td><%= expense.reason_id %></td>
    <td><%= expense.store_id %></td>
    <td><%= expense.user_id %></td>
    <td><%= expense.group_id %></td>
    <td><%= expense.amount %></td>
    <td><%= expense.process_date %></td>
    <td><%= expense.process_flag %></td>
    <td><%= link_to 'Show', expense %></td>
    <td><%= link_to 'Edit', edit_expense_path(expense) %></td>
    <td><%= link_to 'Destroy', expense, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Expense', new_expense_path %>
